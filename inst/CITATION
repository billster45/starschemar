citHeader("To cite starschemar in publications use:")

citEntry(entry = "Manual",
  title        = "{starschemar}: Obtaining a Star Schema from a Flat Table in R",
  author       = personList(as.person("Jose Samos")),
  year         = "2020",
  note         = "R package version 1.0.0",
  url          = "https://github.com/josesamos/starschemar",

  textVersion  =
  paste("Jose Samos (2020).",
        "starschemar: Obtaining a Star Schema from a Flat Table in R.",
        "https://github.com/josesamos/starschemar.")
)
